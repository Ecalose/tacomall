{
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "首页",
        "navigationStyle": "custom",
        "enablePullDownRefresh": true,
        "app-plus": {
          "titleNView": false
        }
      }
    },
    {
      "path": "pages/gallery/index",
      "style": {
        "navigationBarTitleText": "分类",
        "navigationStyle": "custom",
        "enablePullDownRefresh": true,
        "app-plus": {
          "titleNView": false
        }
      }
    },
    {
      "path": "pages/cart/index",
      "style": {
        "navigationBarTitleText": "购物车",
        "navigationStyle": "custom",
        "enablePullDownRefresh": true,
        "enablePullDownRefresh": true,
        "app-plus": {
          "titleNView": false
        }
      }
    },
    {
      "path": "pages/me/index",
      "style": {
        "navigationBarTitleText": "我的",
        "navigationStyle": "custom",
        "enablePullDownRefresh": true,
        "enablePullDownRefresh": true,
        "app-plus": {
          "titleNView": false,
          "bounce": "none"
        }
      }
    },
    {
      "path": "pages/login/index",
      "style": {
        "navigationBarTitleText": "登录",
        "navigationStyle": "custom",
        "enablePullDownRefresh": true,
        "app-plus": {
          "titleNView": false,
          "bounce": "none"
        }
      }
    }
  ],
  "tabBar": {
    "color": "#7A7E83",
    "selectedColor": "#845d32",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "pagePath": "pages/index/index",
        "iconPath": "static/image/tab_home.png",
        "selectedIconPath": "static/image/tab_home--active.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/gallery/index",
        "iconPath": "static/image/tab_gallery.png",
        "selectedIconPath": "static/image/tab_gallery--active.png",
        "text": "分类"
      },
      {
        "pagePath": "pages/cart/index",
        "iconPath": "static/image/tab_cart.png",
        "selectedIconPath": "static/image/tab_cart--active.png",
        "text": "购物车"
      },
      {
        "pagePath": "pages/me/index",
        "iconPath": "static/image/tab_me.png",
        "selectedIconPath": "static/image/tab_me--active.png",
        "text": "个人中心"
      }
    ]
  },
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "塔可商城",
    "navigationBarBackgroundColor": "#fff",
    "backgroundColor": "#F8F8F8",
    "mp-alipay": {
      "titleBarColor": "#FFFFFF"
    },
    "usingComponents": {}
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^cut-(.*)": "codingtalk-uni-toolkit/src/components/cut-$1/cut-$1.vue",
      "^u-(.*)": "uview-plus/components/u-$1/u-$1.vue"
    }
  }
}